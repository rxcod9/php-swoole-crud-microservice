{
  "title": "Swoole HTTP Metrics",
  "panels": [
    {
      "type": "graph",
      "title": "Requests per second",
      "targets": [
        {
          "expr": "rate(http_requests_total_Requests[1m])"
        }
      ]
    },
    {
      "type": "graph",
      "title": "Request Latency (p95)",
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum(rate(http_request_seconds_Latency_bucket[1m])) by (le, path))"
        }
      ]
    }
  ],
  "schemaVersion": 16,
  "version": 1
}
